{
  "name": "superman-shared",
  "version": "0.1.7",
  "description": "the build and test utils share for vue-superman packages",
  "keywords": [
    "superman-shared",
    "vue-superman",
    "utils"
  ],
  "author": "yangjinming <https://github.com/2214962083>",
  "funding": "https://github.com/sponsors/2214962083",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "pnpm type-check &&unbuild",
    "build:watch": "pnpm build -- --stub",
    "clean": "rimraf ./dist/**/*",
    "type-check": "tsc --noEmit"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*",
    "./test-utils": {
      "require": "./dist/test-utils.cjs",
      "import": "./dist/test-utils.mjs",
      "types": "./dist/test-utils.d.ts"
    },
    "./test-vue-utils": {
      "require": "./dist/test-vue-utils.cjs",
      "import": "./dist/test-vue-utils.mjs",
      "types": "./dist/test-vue-utils.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2214962083/vue-superman.git",
    "directory": "packages/superman-shared"
  },
  "bugs": {
    "url": "https://github.com/2214962083/vue-superman/issues"
  },
  "homepage": "https://github.com/2214962083/vue-superman#readme",
  "peerDependencies": {
    "vite": "*",
    "vitest": "*",
    "vue-demi": "*"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "vitest": {
      "optional": true
    },
    "vue-demi": {
      "optional": true
    }
  },
  "dependencies": {
    "@types/node": "^17.0.24",
    "@types/node-fetch": "^2.6.1",
    "@types/rimraf": "^3.0.2",
    "chalk": "4.1.2",
    "fs-extra": "^10.1.0",
    "globby": "11.1.0",
    "msw": "^0.39.2",
    "node-fetch": "^2.6.7",
    "ora": "5.4.1",
    "rimraf": "^3.0.2",
    "vite-plugin-dts": "^1.0.5"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "esno": "^0.14.1",
    "typescript": "4.6.3",
    "unbuild": "^0.7.4",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue": "^3.2.31",
    "vue-demi": "*",
    "vue2": "npm:vue@^2.6.14"
  }
}
